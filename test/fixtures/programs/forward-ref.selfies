# Forward reference example
# This file uses definitions before they are declared
# Should produce warnings but still be valid

[alcohol] = [ethyl][hydroxyl]

# These are defined after being used above
[ethyl] = [C][C]
[hydroxyl] = [O]

# This should work fine (backward reference)
[longer_alcohol] = [propyl][hydroxyl]
[propyl] = [C][C][C]
